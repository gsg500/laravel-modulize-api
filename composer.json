{
    "name": "netcreaties/laravel-modulize-api",
    "description": "Creating easy to use modules without having to configure them.",
    "type": "library",
    "keywords": ["laravel"],
    "require": {},
    "require-dev": {
        "matthewbdaly/artisan-standalone": "0.0.*",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.7.1",
        "orchestra/testbench-browser-kit": "~3.7.1",
        "php-coveralls/php-coveralls": "^2.1",
        "phpstan/phpstan": "^0.10.5",
        "phpunit/phpunit": "^7.0",
        "psy/psysh": "^0.9.8",
        "sebastian/phpcpd": "^4.0",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Roy Freij",
            "email": "info@royfreij.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaravelModulize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelModulize\\Providers\\ModulizeServiceProvider"
            ]
        }
    }
}
